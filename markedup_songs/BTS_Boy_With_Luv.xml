            <writer artistRef="RM">RM</writer>
            <writer>Pdogg</writer>
        </lyricCredit>
        <label labelRef="BigHitEnt">Big Hit Entertainment</label>
        <releaseDate when="2019-04-12">April 12, 2019</releaseDate>
    </about>
    
